			# grc_cr_exp_*_KD*_M*.*

				grc_crexp=() # critical expontents

				grc_crexp1=( `( plo <<EOF2
< "cat ltot/${NU}_*_KD${K}_M${MA}_EL${EL[$el]}_*.ltot

cal gdotmax $gdotmax
